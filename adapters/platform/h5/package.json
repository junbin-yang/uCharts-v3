{
  "name": "ucharts-h5",
  "version": "1.0.0",
  "description": "uCharts H5平台适配器",
  "main": "../../dist/h5/ucharts-h5.min.js",
  "module": "../../dist/h5/ucharts-h5.esm.js",
  "types": "../../dist/h5/types/index.d.ts",
  "scripts": {
    "build": "npm run clean && rollup -c rollup.config.js",
    "build:watch": "rollup -c rollup.config.js -w",
