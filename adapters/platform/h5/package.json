{
  "name": "ucharts-v3",
  "version": "1.0.1-beta",
  "description": "UCharts 是一款基于TypeScript的高性能跨平台数据可视化图表库，支持多种常用图表类型，满足各类应用的数据可视化需求。",
  "main": "./ucharts-v3.min.js",
  "module": "./ucharts-v3.esm.js",
  "types": "./types/index.d.ts",
  "files": [
    "ucharts-v3.js",
    "ucharts-v3.min.js",
    "ucharts-v3.esm.js",
    "*.map",
    "types/",
    "README.md"
  ],
  "scripts": {
    "build": "npm run clean && rollup -c rollup.config.js",
    "clean": "rimraf ../../dist/h5 && rimraf .publish-temp",
    "dev": "rollup -c rollup.config.js --watch",
    "prepare:publish": "node prepare-publish.js",
    "prepublishOnly": "echo 'Skipping build in prepublishOnly - build should be done before publish'",
    "publish:beta": "node publish.js --beta",
    "publish:latest": "node publish.js",
    "pack:check": "npm run prepare:publish && cd .publish-temp && npm pack --dry-run"
  },
  "keywords": [
    "h5",
    "web",
    "chart",
    "canvas",
    "visualization",
    "图表",
    "可视化",
    "ucharts",
    "数据可视化",
    "Web图表"
  ],
  "author": {
    "name": "UCharts Team",
    "email": "282764407@qq.com"
  },
  "contributors": [
    {
      "name": "junbin.yang",
      "email": "282764407@qq.com"
    },
    {
      "name": "qiuyun",
      "email": "174748180@qq.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/junbin-yang/uCharts-v3.git",
    "directory": "adapters/platform/h5"
  },
  "homepage": "https://github.com/junbin-yang/uCharts-v3#readme",
  "bugs": {
    "url": "https://github.com/junbin-yang/uCharts-v3/issues"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "rimraf": "^5.0.5",
    "rollup": "^3.29.5",
    "typescript": "^5.8.3"
  },
  "browserslist": [
    "last 2 versions",
    "not dead"
  ],
  "exports": {
    ".": {
      "import": "./ucharts-v3.esm.js",
      "require": "./ucharts-v3.min.js",
      "types": "./types/index.d.ts"
    }
  }
}